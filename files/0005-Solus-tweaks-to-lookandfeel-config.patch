     <entry name="lookAndFeelPackage" key="LookAndFeelPackage" type="String">
       <label>Global look and feel</label>
-      <default>org.kde.breeze.desktop</default>
+      <default>solusdark.desktop</default>
     </entry>
   </group>
 </kcfg>
